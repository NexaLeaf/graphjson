{
  "name": "@nexaleaf/graphjson",
  "version": "0.0.0",
  "packageManager": "pnpm@10.0.0",
  "license": "MIT",
  "scripts": {
    "build": "nx run-many -t build",
    "build:watch": "nx run-many -t build --watch",
    "test": "nx run-many -t test",
    "test:watch": "nx run-many -t test --watch",
    "test:coverage": "nx run-many -t test --coverage",
    "lint": "nx run-many -t lint",
    "lint:fix": "nx run-many -t lint --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "dev": "nx run-many -t dev --parallel",
    "graph": "nx graph",
    "docs:build": "nx build docs",
    "docs:serve": "nx serve docs",
    "release": "nx release --dry-run",
    "release:publish": "nx release publish",
    "typecheck": "nx run-many -t typecheck",
    "clean": "nx reset && rm -rf dist node_modules"
  },
  "private": false,
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nx/esbuild": "22.4.1",
    "@nx/eslint": "22.4.0",
    "@nx/eslint-plugin": "22.4.0",
    "@nx/jest": "22.4.0",
    "@nx/js": "22.4.1",
    "@nx/node": "^22.4.1",
    "@nx/vite": "22.4.0",
    "@nx/vitest": "22.4.0",
    "@nx/web": "22.4.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/jest": "^30.0.0",
    "@types/node": "20.19.9",
    "@vitest/coverage-v8": "4.0.9",
    "@vitest/ui": "4.0.9",
    "esbuild": "^0.19.2",
    "eslint": "~8.57.0",
    "jest": "^30.0.2",
    "jest-environment-node": "^30.0.2",
    "jest-util": "^30.0.2",
    "jiti": "2.4.2",
    "jsdom": "^27.1.0",
    "jsonc-eslint-parser": "^2.1.0",
    "nx": "22.4.0",
    "prettier": "~3.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "tsx": "^4.21.0",
    "typedoc": "^0.28.16",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.40.0",
    "verdaccio": "^6.0.5",
    "vite": "^7.0.0",
    "vite-plugin-dts": "~4.5.0",
    "vitest": "4.0.9"
  },
  "workspaces": [
    "apps/*",
    "libs/*",
    "examples/*"
  ],
  "nx": {
    "includedScripts": [],
    "targets": {
      "local-registry": {
        "executor": "@nx/js:verdaccio",
        "options": {
          "port": 4873,
          "config": ".verdaccio/config.yml",
          "storage": "tmp/local-registry/storage"
        }
      }
    }
  },
  "dependencies": {
    "axios": "^1.6.0",
    "graphql": "^16.12.0",
    "yargs": "^18.0.0"
  }
}
