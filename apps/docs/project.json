{
  "name": "docs",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/docs",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/apps/docs"],
      "options": {
        "command": "pnpm exec docusaurus build apps/docs --out-dir dist/apps/docs"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec docusaurus start apps/docs --port 3000"
      }
    }
  }
}
