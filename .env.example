# GraphJSON Environment Configuration
# Copy this file to .env.local and fill in your values

# NPM Configuration
# Used for publishing packages to NPM registry
NPM_TOKEN=your_npm_token_here

# GitHub Configuration
# Used for GitHub Actions and API interactions
GITHUB_TOKEN=your_github_token_here

# GraphQL Endpoint (for examples and testing)
# Example GraphQL API endpoint for testing queries
GRAPHQL_ENDPOINT=https://api.example.com/graphql

# Development Configuration
# Set to true for verbose logging
DEBUG=false

# Node Environment
NODE_ENV=development

# Local Registry (Verdaccio)
# Port for local NPM registry when running locally
LOCAL_REGISTRY_PORT=4873

# Documentation
# URL for documentation site
DOCS_URL=http://localhost:3000

# CI/CD Configuration
# Set to true when running in CI environment
CI=false